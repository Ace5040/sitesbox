cd $SCRIPTPATH/..

#docker-compose --project-name webcrate down
docker stop webcrate-core webcrate-admin webcrate-nginx webcrate-tools influxdb grafana solr htmltopdf doctohtml phppgadmin phpmyadmin mysql mysql5 postgres memcached
