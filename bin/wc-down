#!/bin/bash

SCRIPT=`realpath $0`
SCRIPTPATH=`dirname $SCRIPT`

cd $SCRIPTPATH/..

docker stop webcrate-core webcrate webcrate-nginx webcrate-tools influxdb grafana solr htmltopdf doctohtml phppgadmin phpmyadmin mysql mysql5 postgres memcached mattermost
docker container rm webcrate-core webcrate webcrate-nginx webcrate-tools influxdb grafana solr htmltopdf doctohtml phppgadmin phpmyadmin mysql mysql5 postgres memcached mattermost
docker network rm webcrate_network
